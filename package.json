  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2020 browser module main",
    "start": "nx serve",
    "build": "nx build",
    "build:libs": "npm run update:package:deps && ts-node tools/scripts/build/build-libs.ts",
    "test": "nx test",
    "update:versions": "ts-node tools/scripts/update-version-numbers.ts",
    "update:package:deps": "ts-node tools/scripts/update-dependency-versions.ts",
    "build:ng-prototype": "npx nx run nx-ng-mat-prototype:build",
    "copy:charts:scss": "ts-node -e \"import { copyChartScss } from './tools/scripts/copy-charts-scss'; copyChartScss().then(() => {});\"",
    "copy:dl:scss": "ts-node -e \"import { copyDesignLibraryStyles } from './tools/scripts/copy/copy-design-library-scss-config'; copyDesignLibraryStyles().then(() => {});\"",
    "ts-node:config": "ts-node --showConfig",
    "schematics:build": "npx nx run schematics:build",
    "charts:build": "npx nx run packages-charts:build && npm run copy:charts:scss",
    "fn:build": "npx nx run fn:build",
    "fn:test": "npx nx run fn:test",
    "date:build": "npx nx run date:build",
    "date:test": "npx nx run date:test",
    "charts:test": "npx nx run charts:test",
    "publish:latest": "ts-node tools/scripts/publish/publish-latest.ts",
    "chmod": "node ./scripts/chmod packages/nx-ng-mat-prototype/bin/generate.sh",
    "secrets": "bash scripts/secrets.sh ng-patterns_private",
    "d.patterns.prd": "bash scripts/deploy.ng-patterns.prd.sh",
    "compodoc": "compodoc -p tsconfig.doc.json -s",
    "mat-code": "ts-node tools/scripts/list-material-components.ts"
  },
  "private": true,
