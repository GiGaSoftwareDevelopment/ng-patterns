{
  "name": "charts",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/charts/src",
  "prefix": "ng-patterns",
  "targets": {
    "build": {
      "executor": "@nrwl/angular:package",
      "outputs": ["{workspaceRoot}/dist/libs/charts"],
      "options": {
        "project": "libs/charts/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/charts/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/charts/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/libs/charts"],
      "options": {
        "jestConfig": "libs/charts/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/charts/**/*.ts",
          "libs/charts/**/*.html",
          "libs/charts/bullet-chart/**/*.ts",
          "libs/charts/bullet-chart/**/*.html",
          "libs/charts/line-chart/**/*.ts",
          "libs/charts/line-chart/**/*.html"
        ]
      }
    }
  },
  "tags": []
}
