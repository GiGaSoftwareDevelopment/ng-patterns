{
  "name": "nx-ng-mat-prototype",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/nx-ng-mat-prototype/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/nx-ng-mat-prototype",
        "main": "libs/nx-ng-mat-prototype/src/index.ts",
        "tsConfig": "libs/nx-ng-mat-prototype/tsconfig.lib.json",
        "assets": [
          "libs/nx-ng-mat-prototype/*.md",
          "libs/nx-ng-mat-prototype/bin/*.sh"
        ]
      }
    },
    "publish": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node tools/scripts/publish.mjs nx-ng-mat-prototype {args.ver} {args.tag}"
      },
      "dependsOn": [
        {
          "projects": "self",
          "target": "build"
        }
      ]
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/nx-ng-mat-prototype/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/libs/nx-ng-mat-prototype"],
      "options": {
        "jestConfig": "libs/nx-ng-mat-prototype/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
