{
  "generators": {
    "design-library-component": {
      "factory": "./src/generators/design-library-component/generator",
      "schema": "./src/generators/design-library-component/schema.json",
      "description": "Create a design library component."
    },
    "feature-component": {
      "factory": "./src/generators/feature-component/generator",
      "schema": "./src/generators/feature-component/schema.json",
      "description": "Create a feature component."
    },
    "ngrx-effect-store": {
      "factory": "./src/generators/ngrx-effect-store/generator",
      "schema": "./src/generators/ngrx-effect-store/schema.json",
      "description": "Create an Ngrx entity store."
    },
    "ngrx-entity": {
      "factory": "./src/generators/ngrx-entity/generator",
      "schema": "./src/generators/ngrx-entity/schema.json",
      "description": "Create an Ngrx store with Effect."
    },
    "domain": {
      "factory": "./src/generators/domain/generator",
      "schema": "./src/generators/domain/schema.json",
      "description": "Create app and libraries in a domain"
    },
    "add-material-to-project": {
      "factory": "./src/generators/add-material-to-project/generator",
      "schema": "./src/generators/add-material-to-project/schema.json",
      "description": "Add Material to a project"
    },
    "add-app-to-domain": {
      "factory": "./src/generators/add-app-to-domain/generator",
      "schema": "./src/generators/add-app-to-domain/schema.json",
      "description": "Add app and libraries to established domain."
    }
  }
}
